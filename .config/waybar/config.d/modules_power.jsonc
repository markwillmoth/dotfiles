{
  "group/power-menu": {
    "drawer": {
      "click-to-reveal": true,
      "transition-left-to-right": false,
    },
    "orientation": "horizontal",
    "modules": [
      "custom/power-menu-icon",
      "custom/power-lock",
      "custom/power-logout",
      "custom/power-suspend",
      "custom/power-reboot",
      "custom/power-shutdown",
    ],
  },
  "custom/power-menu-icon": {
    "format": "",
  },
  "custom/power-lock": {
    "format": "",
    "on-click": "~/.config/waybar/scripts/power_lock.sh",
  },
  "custom/power-logout": {
    "format": "",
    "on-click": "~/.config/waybar/scripts/power_logout.sh",
  },
  "custom/power-suspend": {
    "format": "",
    "on-click": "~/.config/waybar/scripts/power_suspend.sh",
  },
  "custom/power-reboot": {
    "format": "",
    "on-click": "~/.config/waybar/scripts/power_reboot.sh",
  },
  "custom/power-shutdown": {
    "format": "",
    "on-click": "~/.config/waybar/scripts/power_shutdown.sh",
  },
}
